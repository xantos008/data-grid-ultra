{
  "name": "data-grid-ultra",
  "version": "4.7.0",
  "description": "A react data table implementation using @mui.",
  "author": "xantos008",
  "files": [
    "dist/*"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/xantos008/data-grid-ultra/issues"
  },
  "homepage": "https://github.com/xantos008/data-grid-ultra",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "react-component",
    "material-ui",
    "mui",
    "react-table",
    "table",
    "datatable",
    "data-table",
    "datagrid",
    "data-grid"
  ],
  "scripts": {
    "compile": "rm -rf build && tsc && cp package.json build && cp README.md build && cp LICENSE build",
    "clean": "rm -rf dist",
    "build:simple": "npm run clean && rollup -c --bundleConfigAsCjs",
    "build": "npm run clean && rollup -c --bundleConfigAsCjs && npm run postbuild",
    "postbuild": "cp package.json dist && cp README.md dist && cp LICENSE dist && node ./scripts/runUpdatePackageJson.js",
    "publish": "npm run compile && npm run build && npm run pack && npm publish data-grid-ultra-4.7.0.tgz",
    "pack": "npm pack ./build/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xantos008/data-grid-ultra.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.7",
    "@emotion/styled": "^11.11.5",
    "@mui/system": "^5.15.15",
    "@mui/utils": "^5.15.14",
    "@mui/x-data-grid": "7.7.0",
    "@types/format-util": "^1.0.4",
    "clsx": "^2.1.1",
    "data-grid-extra": "4.7.0",
    "exceljs": "^4.4.0",
    "prop-types": "^15.8.1",
    "reselect": "^4.1.8",
    "semver": "^7.5.4"
  },
  "peerDependencies": {
    "@mui/material": "^5.15.14",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/traverse": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-url": "^8.0.2",
    "@types/node": "^18.19.34",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.3",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "csstype": "3.0.10",
    "@babel/traverse": "^7.24.7"
  }
}
